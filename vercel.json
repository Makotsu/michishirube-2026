{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "schedule.michishirube.okinawa"
        }
      ],
      "destination": "https://shiori.michishirube.okinawa/:path*",
      "permanent": true
    }
  ]
}
